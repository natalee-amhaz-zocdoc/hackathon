{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "LocalStack": {
    "UseLocalStack": true,
    "Session": {
      "AwsAccessKeyId": "ci-AwsAccessKeyId-ci",
      "AwsAccessKey": "ci-AwsAccessKey-ci",
      "AwsSessionToken": "ci-AwsSessionToken-ci",
      "RegionName": "us-east-1"
    },
    "Config": {
      "LocalStackHost": "localstack",
      "UseSsl": false,
      "UseLegacyPorts": false,
      "EdgePort": 4566
    }
  }
}
