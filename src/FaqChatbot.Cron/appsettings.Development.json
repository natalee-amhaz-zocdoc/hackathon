{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "LocalStack": {
    "UseLocalStack": true,
    "Session": {
      "AwsAccessKeyId": "local-AwsAccessKeyId-ci",
      "AwsAccessKey": "local-AwsAccessKey-ci",
      "AwsSessionToken": "local-AwsSessionToken-ci",
      "RegionName": "us-east-1"
    },
    "Config": {
      "LocalStackHost": "localhost",
      "UseSsl": false,
      "UseLegacyPorts": false,
      "EdgePort": 4566
    }
  },
  "Zipkin": {
    "Endpoint": "http://localhost:9411"
  }
}
