{
  "Zocdoc": {
    "Http": {},
    "ServiceName": "FaqChatbot"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Sentry": {
    "IncludeRequestPayload": false,
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Error",
    "AttachStackTrace": true,
    "Debug": true,
    "DiagnosticsLevel": "Error",
    "Environment": "Production"
  },
  "Kestrel": {
    "Endpoints": {
      "https": {
        "Url": "https://+:443",
        "Port": "443"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "/etc/ssl/server.pfx",
        "AllowInvalid": true
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Sentry": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Sentry",
        "Args": {
          "MinimumBreadcrumbLevel": "Debug",
          "MinimumEventLevel": "Warning"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ]
  },
  "Zipkin": {
    "Endpoint": "https://otel.east.zocdoccloud.com"
  }
}
