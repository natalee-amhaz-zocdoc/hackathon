{
  "Zocdoc": {
    "Http": {},
    "ServiceName": "FaqChatbot"
  },
  "Kestrel": {
    "Endpoints": {
      "http": {
        "Url": "http://+:80",
        "Port": "80"
      },
      "https": {
        "Url": "https://+:443",
        "Port": "443"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "/etc/ssl/server.pfx",
        "AllowInvalid": true
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "LocalStack": {
    "UseLocalStack": true,
    "Session": {
      "AwsAccessKeyId": "ci-AwsAccessKeyId-ci",
      "AwsAccessKey": "ci-AwsAccessKey-ci",
      "AwsSessionToken": "ci-AwsSessionToken-ci",
      "RegionName": "us-east-1"
    },
    "Config": {
      "LocalStackHost": "localstack",
      "UseSsl": false,
      "UseLegacyPorts": false,
      "EdgePort": 4566
    }
  },
  "DynamoDBTableNamePrefix": "dev-"
}
